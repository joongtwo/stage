{
    "schemaVersion": "1.0.0",
    "props": {
        "view-path": {
            "type": "object"
        },
        "view-id": {
            "type": "string"
        },
        "sub-panel-context": {
            "type": "object"
        }
    },
    "data": {
        "customColumns":
        [
            {
                "clientColumn": true,
                "name": "graphicVisibility",
                "propertyDisplayName": "{{i18n.graphicVisibilityColumnName}}"
            }
        ]
    },

    "actions": {
        "getValueInViewModel": {
            "actionType": "JSFunction",
            "method": "getValueInViewModel",
            "inputData": {
                "tabData": "{{subPanelContext}}"
            },
            "outputData": {
                "tabContext": ""
            },
            "deps": "js/mfeViewModelUtils"
        }
    },

    "lifecycleHooks": {
        "onMount": "getValueInViewModel"
    },

    "i18n": {
        "graphicVisibilityColumnName":[ "TablesMessages" ]
    }
}
