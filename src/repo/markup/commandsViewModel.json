  "conditions": {
    "markupVisible": {
      "expression": "commandContext.type=='aw-pdf-viewer' || commandContext.type=='aw-2d-viewer' || commandContext.type=='aw-image-viewer' || commandContext.type=='aw-text-viewer' || commandContext.type=='aw-html-viewer' ||  ctx.reqMarkupCtx.viewerType=='aw-requirement-ckeditor' || ctx.markup.keep"
    },
    "markupOnViewer": {
      "expression": "commandContext.element===ctx.markup.viewerElement"
    },
    "markupShowMarkupsSelected": {
      "expression": "ctx.markup.showMarkups && conditions.markupOnViewer"
    },
    "markupShowPanelSelected": {
      "expression": "ctx.markup.showPanel && conditions.markupOnViewer"
    },
    "markupHighlightEnabled": {
      "expression": "!ctx.markup.editing && ctx.markup.supportedTools.highlight && conditions.markupOnViewer"
    },
    "markupFreehandEnabled": {
      "expression": "!ctx.markup.editing && !ctx.markup.playing && ctx.markup.supportedTools.freehand && conditions.markupOnViewer"
    },
    "markupHighlightSelected": {
      "expression": "ctx.markup.selectedTool==='highlight' && conditions.markupOnViewer"
    },
    "markupFreehandSelected": {
      "expression": "ctx.markup.selectedTool==='freehand' && conditions.markupOnViewer"
    },
    "markupShapeSelected": {
      "expression": "ctx.markup.selectedTool==='shape' && conditions.markupOnViewer"
    },
    "markupShapeEnabled": {
      "expression": "!ctx.markup.editing && !ctx.markup.playing && ctx.markup.supportedTools.shape && conditions.markupOnViewer"
    },
    "markupStampSelected": {
      "expression": "ctx.markup.selectedTool==='stamp' && conditions.markupOnViewer"
    },
    "markupStampEnabled": {
      "expression": "!ctx.markup.editing && !ctx.markup.playing && ctx.markup.supportedTools.stamp && conditions.markupOnViewer"
    },
    "markupEditable": {
      "expression": "commandContext.vmo.isEditable"
    },
    "markupReplyable": {
      "expression": "commandContext.vmo.isReplyable"
    },
    "markupDeletable": {
      "expression": "commandContext.vmo.isDeletable"
    },
    "markupPrintable": {
      "expression": "(ctx.markup.showMarkups || ctx.markup.showPanel) && commandContext.type !== 'aw-2d-viewer' && ctx.markup.count > 0 && conditions.markupOnViewer"
    },
    "markupGdntRemovable": {
      "expression": "commandContext.rowCount > 1"
    }
  },
