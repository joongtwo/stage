  "actions": {
    "ssp0NavigateToTimeAnalysisCommandAction": {
      "actionType": "Navigate",
      "navigateTo": "timeAnalysis",
      "navigationParams": {
        "uid": "{{ctx.selectedVMO.uid}}"
      }
    },
    "navigateToServicePlanPage": {
      "actionType": "Navigate",
      "navigateTo": "servicePlanning",
      "navigationParams": {
        "uid": {
          "$adapt": "{{commandContext.vmo}}",
          "path": "uid"
        }
      }
    },
    "ssp0AddServiceContainerAction": {
      "actionType": "popup",
      "method": "show",
      "inputData": {
        "options": {
          "caption": "{{i18n.createServiceContainerTitle}}",
          "view": "Ssp0CreateServiceContainer",
          "preset": "modal",
          "height": "420",
          "width": "400",
          "clickOutsideToClose": false,
          "draggable": true,
          "enableResize": true,
          "independent": true
        }
      }
    },
    "ssp0AddServiceRequirementAction": {
      "actionType": "popup",
      "method": "show",
      "inputData": {
        "options": {
          "caption": "{{i18n.createServiceReqTitle}}",
          "view": "Ssp0CreateServiceReq",
          "preset": "modal",
          "height": "420",
          "width": "400",
          "clickOutsideToClose": false,
          "draggable": true,
          "enableResize": true,
          "independent": true
        }
      }
    },
    "AddFrequency": {
      "actionType": "popup",
      "inputData": {
        "options": {
          "caption": "{{i18n.CreateFrequency}}",
          "view": "Ssp1AddFrequency",
          "ignoreLimit": true,
          "preset": "modal",
          "width": "1400",
          "height": "700",
          "hasCloseButton": true,
          "clickOutsideToClose": false,
          "draggable": true,
          "enableResize": true,
          "independent": true,
          "resizable": true
        }
      }
    },
    "AddFrequencyToListAdapter": {
      "actionType": "Event",
      "method": "Event",
      "inputData": {
        "events": [
          {
            "name": "ssp1AddFrequency.AddFrequencyToList"
          }
        ]
      }
    },
    "ssp0CreateWorkCardAction": {
      "actionType": "popup",
      "method": "show",
      "inputData": {
        "options": {
          "caption": "{{i18n.addWorkCard}}",
          "view": "Ssp0CreateWorkCard",
          "preset": "modal",
          "height": "420",
          "width": "410",
          "clickOutsideToClose": false,
          "draggable": true,
          "enableResize": true,
          "independent": true
        }
      }
    },
    "RemoveFrequencyExpressionAdapter": {
      "actionType": "Event",
      "method": "Event",
      "inputData": {
        "events": [
          {
            "name": "ssp1AddFrequency.removeExpressionLine",
            "eventData": {
              "exprId": "{{commandContext.vmo.Title}}"
            }
          }
        ]
      }
    },
    "MoveUpFrequencyExpressionAdapter": {
      "actionType": "Event",
      "method": "Event",
      "inputData": {
        "events": [
          {
            "name": "ssp1AddFrequency.moveUpExpressionLine",
            "eventData": {
