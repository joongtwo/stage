            }
          }
        ]
      }
    },
    "MoveDownFrequencyExpressionAdapter": {
      "actionType": "Event",
      "method": "Event",
      "inputData": {
        "events": [
          {
            "name": "ssp1AddFrequency.moveDownExpressionLine",
            "eventData": {
              "exprId": "{{commandContext.vmo.Title}}"
            }
          }
        ]
      }
    },
    "ResetFrequencyFieldsAdapter": {
      "actionType": "Event",
      "method": "Event",
      "inputData": {
        "events": [
          {
            "name": "ssp1AddFrequency.resetFrequencyFields"
          }
        ]
      }
    },
    "GroupFrequencyAdapter": {
      "actionType": "Event",
      "method": "Event",
      "inputData": {
        "events": [
          {
            "name": "ssp1AddFrequency.groupFrequencies"
          }
        ]
      }
    },
    "UngroupFrequencyAdapter": {
      "actionType": "Event",
      "method": "Event",
      "inputData": {
        "events": [
          {
            "name": "ssp1AddFrequency.ungroupFrequencies"
          }
        ]
      }
    },
    "ssp0addActivityCommandAction": {
      "actionType": "popup",
      "method": "show",
      "inputData": {
        "options": {
          "caption": "Activity",
          "view": "Ssp0CreateActivity",
          "preset": "modal",
          "height": "420",
          "width": "400",
          "clickOutsideToClose": false,
          "draggable": true,
          "enableResize": true,
          "independent": true
        }
      }
    },
    "ssp0removeActivityCommandAction": {
      "actionType": "JSFunction",
      "method": "removeActivity",
      "inputData": {
        "createType": "CfgActivityLine"
      },
      "deps": "js/ssp0ActivityUtilityService"
    }
  },
  "conditions": {
    "isWCSelected": {
      "expression": "ctx.selectedVMO.modelType.typeHierarchyArray.includes('SSP0BvrWorkCard')"
    },
    "ssp0ShowSPCondition": {
      "expression": "(ctx.mselected[0].modelType.typeHierarchyArray.includes('SSP0ServicePlan') && commandContext.vmo.modelType.typeHierarchyArray.includes( 'Awp0XRTObjectSetRow')) || (ctx.pselected.modelType.typeHierarchyArray.includes('SSP0ServicePlan'))"
    },
    "isServicePlanSelected": {
      "expression": "ctx.selectedVMO !== null"
    },
    "isServiceRequirementSelected": {
      "expression": "ctx.selectedVMO.modelType.typeHierarchyArray.includes('SSP0BvrServiceRequirement')"
    },
    "addFrequencyCommandActive": {
      "expression": "(ctx.selectedVMO.modelType.typeHierarchyArray.includes('SSP0BvrServiceRequirement'))"
    },
    "isServicePlanOrServiceContainerSelected": {
      "expression": "(ctx.selectedVMO.modelType.typeHierarchyArray.includes('SSP0BvrServicePlan')) || (ctx.selectedVMO.modelType.typeHierarchyArray.includes( 'SSP0BvrServiceContainer'))"
    },
    "isAddActivitySelected": {
      "expression": "ctx.selectedActivity!== undefined"
    },
    "isRemoveActivitySelected": {
      "expression": "ctx.selectedActivity!== undefined && (ctx.selectedActivity.props.me_cl_parent.dbValues[0]!=='' )"
    }
  },
