    },
    "senOpenCreateWorkPackage": {
      "actionType": "popup",
      "inputData": {
        "options": {
          "view": "EpCreateObject",
          "preset": "modal",
          "caption": "{{i18n.senCreateWorkPackageHeaderTitle}}",
          "height": "500",
          "width": "450",
          "independent": true,
          "subPanelContext": {
            "objectBaseType": "MECollaborationContext",
            "subTypeExclusionList": [
              "Mei0ExecutionPlan",
              "Ppe0PcbWorkPackage",
              "Ap0AdvanceCollabContext",
              "Ppe0ElectronicsWorkPackage",
              "Mbc0InternalWorkPackage",
              "Mei0ProductionPlan"
            ],
            "hideCreateAndAddMoreButton": true
          }
        }
      },
      "outputData": {
        "popupId": ""
      }
    },
    "showConfirmationMessageForRemoveFromSbomContext": {
      "actionType": "JSFunction",
      "method": "getDisplayName",
      "deps": "js/typeDisplayName.service",
      "inputData": {
        "modelObject": "{{ctx.sbomContext.selectedModelObjects[0]}}"
      },
      "outputData": {
        "displayNameOfSelectedElement": ""
      },
      "actionMessages": {
        "success": [
          {
            "condition": "ctx.sbomContext.selectedModelObjects.length === 1",
            "message": "removeSingleFromSbom"
          },
          {
            "condition": "ctx.sbomContext.selectedModelObjects.length > 1",
            "message": "removeMultiplefromSbom"
          }
        ]
      }
    },
    "removeAndUpdateElements": {
      "actionType": "batchJob",
      "steps": [
        {
          "action": "removeElements"
        },
        {
          "action": "updateCompareInfo"
        }
      ]
    },
    "updateCompareInfo": {
      "actionType": "JSFunction",
      "method": "updateCompareInfo",
      "deps": "js/senCompareUtils"
    },
    "senRevisePreProcessAction": {
      "actionType": "JSFunction",
      "method": "preprocessAndCreateReviseInput",
      "deps": "js/senReviseService",
      "inputData": {
        "selectedObject": "{{ctx.mselected}}",
        "parent": "{{ctx.taskPageContext.cnObject.uid}}"
      },
      "outputData": {
        "reviseInput": ""
      },
      "events": {
        "success": [
          {
            "name": "sen.revisePreProcessSucessEvent"
          }
        ]
      }
    },
    "senReviseSoaCallAction": {
      "actionType": "TcSoaService",
      "serviceName": "Core-2013-05-DataManagement",
      "method": "reviseObjects",
      "inputData": {
        "reviseIn": "{{data.reviseInput}}"
      },
      "outputData": {
        "revisedObjects": "reviseTrees",
        "partialErorrs": "partialErrors"
      },
      "events": {
        "success": [
          {
            "name": "sen.reviseSoaSuccess",
            "eventData": {
              "selectedObjects": "{{ctx.mselected}}",
              "revisedObjects": "{{data.revisedObjects}}"
            }
          }
        ]
      },
      "actionMessages": {
        "failure": [
          {
            "message": "reviseFailureMsg",
            "condition": "errorCode.code===48071 || errorCode.code===51003 || errorCode.code===48020"
          }
        ]
      },
      "deps": "js/Awp0ShowSaveAsService"
    },
    "senPostReviseSoaSuccess": {
      "actionType": "JSFunction",
      "method": "reloadRevisedObjects",
      "inputData": {
        "eventData": "{{data.eventData}}"
      },
      "deps": "js/senReviseService"
    },
    "removeNBOMFromWorkpackage": {
      "actionType": "JSFunctionAsync",
      "method": "removeNBOM",
      "inputData": {
        "selectedObject": "{{ctx.epTaskPageContext.EbomStructureContext}}"
      },
      "deps": "js/removeNBOMService"
    }
  },
  "messages": {
    "removeSingleFromSbom": {
      "messageType": "WARNING",
      "messageText": "{{i18n.senRemoveSingleConfirmation}}",
      "navigationOptions": [
        {
          "option": "Cancel",
          "text": "{{i18n.cancel}}"
        },
        {
          "option": "Remove",
          "text": "{{i18n.aceRemove}}",
          "action": "removeAndUpdateElements"
        }
      ],
      "messageTextParams": [
        "{{data.displayNameOfSelectedElement}}"
      ]
    },
    "removeMultiplefromSbom": {
      "messageType": "WARNING",
      "messageText": "{{i18n.senRemoveMultipleConfirmation}}",
      "navigationOptions": [
        {
          "option": "Cancel",
          "text": "{{i18n.cancel}}"
        },
        {
          "option": "Remove",
          "text": "{{i18n.aceRemove}}",
          "action": "removeAndUpdateElements"
        }
      ],
      "messageTextParams": [
        "{{ctx.sbomContext.selectedModelObjects.length}}"
      ]
    },
    "senShowReviseConfirmationMessage": {
      "messageType": "WARNING",
      "messageText": "{{i18n.senReviseElementConfirmation}}",
      "navigationOptions": [
        {
          "option": "Cancel",
          "text": "{{i18n.cancel}}"
        },
        {
          "option": "Revise",
          "text": "{{i18n.senReviseCommandTitle}}",
          "action": "senRevisePreProcessAction"
        }
      ],
      "messageTextParams": [
        "{{ctx.sbomContext.selectedModelObjects[0].props.object_string.uiValues[0]}}"
      ]
    },
    "senShowReviseMultipleElementsConfirmationMessage": {
      "messageType": "WARNING",
      "messageText": "{{i18n.senReviseMultipleElementsConfirmation}}",
      "navigationOptions": [
        {
          "option": "Cancel2",
          "text": "{{i18n.cancel}}"
        },
        {
          "option": "Revise",
          "text": "{{i18n.senReviseCommandTitle}}",
          "action": "senRevisePreProcessAction"
        }
      ],
      "messageTextParams": [
        "{{ctx.sbomContext.selectedModelObjects.length}}"
      ]
    }
  },
  "onEvent": [
    {
      "eventId": "sen.revisePreProcessSucessEvent",
      "action": "senReviseSoaCallAction"
    },
    {
      "eventId": "sen.reviseSoaSuccess",
      "action": "senPostReviseSoaSuccess",
      "cacheEventData": true
    }
  ],
  "conditions": {
    "senOpenWpCmdCondition": {
      "expression": {
        "$and": [
          {
            "$source": {
              "$adapt": "commandContext.vmo"
            },
            "$query": {
              "$source": "modelType.typeHierarchyArray",
              "$query": {
                "$in": [
                  "CCObject"
                ]
              }
            }
          },
          {
            "$source": "conditions.IsSERoleSelected",
            "$query": {
              "$eq": true
            }
          },
          {
            "$source": "conditions.cmdAwp0ShowObjectCellBaseVisibleCondition",
            "$query": {
              "$eq": true
            }
          }
        ]
      }
    },
    "senCCObjectOpenBaseVisibleCondition": {
      "expression": "(conditions.IsSERoleSelected && ctx.selected  && ctx.mselected && ctx.mselected.length === 1 && ctx.selected.modelType.typeHierarchyArray.indexOf( 'CCObject' ) > -1)"
    },
    "senOpenWPInNewTabOrWindowCmdCondition": {
      "expression": "(conditions.senCCObjectOpenBaseVisibleCondition && !ctx.aw_hosting_enabled)"
    },
    "IsSERoleSelected": {
      "expression": "ctx.userSession.props.role_name.dbValues[0] === 'Service Engineer'"
    },
    "issenPackAllCmdActive": {
      "expression": "ctx[commandContext.viewKey].supportedFeatures.Awb0PackSimilarElementsFeature && !ctx[commandContext.viewKey].supportedFeatures['4GStructureFeature'] && commandContext.viewKey === 'ebomContext'||commandContext.viewKey === 'mbomContext'"
    },
    "issenPackAllCmdVisible": {
      "expression": "ctx.tcSessionData.tcMajorVersion>=12 && ctx.tcSessionData.tcMinorVersion>=4"
    },
    "issenUnpackAllCmdActive": {
      "expression": "ctx[commandContext.viewKey].supportedFeatures.Awb0PackSimilarElementsFeature && !ctx[commandContext.viewKey].supportedFeatures['4GStructureFeature'] && commandContext.viewKey === 'ebomContext'||commandContext.viewKey === 'mbomContext'"
    },
    "issenUnpackAllCmdVisible": {
      "expression": "ctx.tcSessionData.tcMajorVersion>=12 && ctx.tcSessionData.tcMinorVersion>=4"
    },
    "isSenPackCmdEnabled": {
      "expression": "commandContext.occContext.supportedFeatures.Awb0PackSimilarElementsFeature && ((ctx.ebomContext.selectedModelObjects[0].props.awb0Parent.dbValues[0] !== null && conditions.isSenEbomContext) || (ctx.sbomContext.selectedModelObjects[0].props.awb0Parent.dbValues[0] !== null && conditions.isSbomContext))"
    },
    "isSenUnpackCmdEnabled": {
      "expression": "commandContext.occContext.supportedFeatures.Awb0PackSimilarElementsFeature && ((ctx.ebomContext.selectedModelObjects[0].props.awb0Parent.dbValues[0] !== null && conditions.isSenEbomContext) || (ctx.sbomContext.selectedModelObjects[0].props.awb0Parent.dbValues[0] !== null && conditions.isSbomContext))"
    },
    "isEBOMorSbomContext": {
      "expression": "commandContext.viewKey === 'ebomContext'||commandContext.viewKey === 'sbomContext'"
    },
    "isSenEbomContext": {
      "expression": "commandContext.viewKey === 'ebomContext'"
    },
    "isSbomContext": {
      "expression": "commandContext.viewKey === 'sbomContext'"
    },
    "isTypeOfAwb0PositionedElementSelectedInEbom": {
      "expression": "ctx.ebomContext.selectedModelObjects[0].modelType && ctx.ebomContext.selectedModelObjects[0].modelType.typeHierarchyArray.indexOf('Awb0PositionedElement') > -1"
    },
    "isTypeOfAwb0PositionedElementSelectedInSbom": {
      "expression": "ctx.sbomContext.selectedModelObjects[0].modelType && ctx.sbomContext.selectedModelObjects[0].modelType.typeHierarchyArray.indexOf('Awb0PositionedElement') > -1 "
    },
    "isAddSbomChildCmdEnable": {
      "expression": "conditions.isTypeOfAwb0PositionedElementSelectedInSbom && commandContext.viewKey === 'sbomContext' && ctx.sbomContext.selectedModelObjects.length ===1"
    },
    "isAddSbomSiblingCmdEnable": {
      "expression": "conditions.isTypeOfAwb0PositionedElementSelectedInSbom && commandContext.viewKey === 'sbomContext' && ctx.sbomContext.selectedModelObjects.length ===1 &&  ctx.mselected[0].props.awb0Parent.dbValues[0] !== null"
    },
    "isAddSbomSubstituteCmdEnable": {
      "expression": "conditions.isTypeOfAwb0PositionedElementSelectedInSbom && commandContext.viewKey === 'sbomContext' && ctx.sbomContext.selectedModelObjects.length ===1 &&  ctx.mselected[0].props.awb0Parent.dbValues[0] !== null"
    },
    "isAddSbomAlternateCmdEnable": {
      "expression": "conditions.isTypeOfAwb0PositionedElementSelectedInSbom && commandContext.viewKey === 'sbomContext' && ctx.sbomContext.selectedModelObjects.length ===1"
    },
    "isRemoveSbomCmdEnable": {
      "expression": "conditions.isTypeOfAwb0PositionedElementSelectedInSbom && commandContext.viewKey === 'sbomContext' && ctx.sbomContext.selectedModelObjects[0].props.awb0Parent.dbValues[0] !== null"
    },
    "isSbomTreeTableEditable": {
      "expression": "conditions.isSbomContext && ctx.sbomDataProvider && ctx.sbomDataProvider.canEdit && !ctx.sbomDataProvider._editing"
    },
    "isSbomTreeTableEditInProgress": {
      "expression": "conditions.isSbomContext && ctx.sbomDataProvider && ctx.sbomDataProvider._editing"
    },
    "issenAssignFromEbomToSbomCmdVisible": {
      "expression": "conditions.isSbomContext"
    },
    "issenAssignFromEbomToSbomCmdEnable": {
      "expression": "conditions.isTypeOfAwb0PositionedElementSelectedInEbom && conditions.isTypeOfAwb0PositionedElementSelectedInSbom && commandContext.viewKey === 'sbomContext' && ctx.ebomContext.selectedModelObjects.length > 0 &&  ctx.sbomContext.selectedModelObjects.length === 1"
    },
    "isOpenSbomObjectCmdActive": {
      "expression": "conditions.isSbomContext"
    },
    "isOpenSbomObjectCmdEnabled": {
      "expression": "conditions.isOpenSbomObjectCmdActive && ctx.sbomContext.selectedModelObjects.length === 1"
    },
    "IsSEWorkspaceSelected": {
      "expression": "ctx.workspace.workspaceId=== 'ServiceEngineeringWorkspace'"
    },
    "senCreateWpCmdCondition": {
      "expression": "(conditions.IsSEWorkspaceSelected && conditions.IsSERoleSelected)"
    },
    "enableOpenSETaskCommand": {
      "expression": "(conditions.IsSERoleSelected && ctx.epTaskPageContext.ebomStructure  && ctx.epTaskPageContext.mbomStructure)"
    },
    "senSubmitToWorkflowCommandEnable": {
      "expression": "conditions.isTypeOfAwb0PositionedElementSelectedInSbom && ctx.sbomContext.selectedModelObjects.length > 0"
    },
    "senReviseCommandEnable": {
      "expression": "conditions.isTypeOfAwb0PositionedElementSelectedInSbom && commandContext.viewKey === 'sbomContext' && ctx.sbomContext.selectedModelObjects[0].props.awb0Parent.dbValues[0] !== null"
    },
    "senOccurrenceEffectivityCommandEnable": {
      "expression": "conditions.isTypeOfAwb0PositionedElementSelectedInSbom && commandContext.viewKey === 'sbomContext' && ctx.sbomContext.selectedModelObjects[0].props.awb0Parent.dbValues[0] !== null"
    },
    "isNBOMSelectedForRemoveStructureContext": {
      "expression": "ctx.epTaskPageContext.EbomStructureContext"
    }
  },
