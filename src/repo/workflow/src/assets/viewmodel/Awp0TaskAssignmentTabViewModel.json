            {
                "types":
                [
                    {
                        "name": "EPMSignoffProfile",
                        "properties":
                        [
                            {
                                "name": "number_of_signoffs"
                            },
                            {
                                "name": "allow_subgroups"
                            },
                            {
                                "name": "group",
                                "modifiers": [ {
                                    "name": "withProperties",
                                    "Value": "true"
                                } ]
                            },
                            {
                                "name": "role",
                                "modifiers": [ {
                                    "name": "withProperties",
                                    "Value": "true"
                                } ]
                            }
                        ]
                    },
                    {
                        "name": "Role",
