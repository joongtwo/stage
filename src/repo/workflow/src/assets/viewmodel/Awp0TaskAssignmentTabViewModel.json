        },
        "isNonReviewAckRouteTaskTemplateSelected" : {
            "expression":  "props.subPanelContext.workflowDgmState.selectedObject && props.subPanelContext.workflowDgmState.selectedObject.modelType.typeHierarchyArray.indexOf( 'EPMTaskTemplate' ) > -1  && (props.subPanelContext.workflowDgmState.selectedObject.modelType.typeHierarchyArray.indexOf( 'EPMReviewTaskTemplate' ) <= -1 && props.subPanelContext.workflowDgmState.selectedObject.modelType.typeHierarchyArray.indexOf( 'EPMAcknowledgeTaskTemplate' ) <= -1 && props.subPanelContext.workflowDgmState.selectedObject.modelType.typeHierarchyArray.indexOf( 'EPMRouteTaskTemplate' ) <= -1 )"
        },
        "isAutoCompleteSignoffOption" : {
            "expression":  "data.isAutoCompleteOptionPresent && props.subPanelContext.workflowDgmState.selectedObject && (props.subPanelContext.workflowDgmState.selectedObject.modelType.typeHierarchyArray.indexOf( 'EPMReviewTaskTemplate' ) > -1 || props.subPanelContext.workflowDgmState.selectedObject.modelType.typeHierarchyArray.indexOf( 'EPMAcknowledgeTaskTemplate' ) > -1 || props.subPanelContext.workflowDgmState.selectedObject.modelType.typeHierarchyArray.indexOf( 'EPMRouteTaskTemplate' ) > -1 )"
        },
        "isNumericQuorum":
        {
            "expression": "data.quorumOptions.dbValue == false"
        },
        "isPercentQuorum":
        {
            "expression": "data.quorumOptions.dbValue == true"
        },
        "isACKTaskTemplateSelected" : {
            "expression":  "props.subPanelContext.workflowDgmState.selectedObject && props.subPanelContext.workflowDgmState.selectedObject.modelType.typeHierarchyArray.indexOf( 'EPMAcknowledgeTaskTemplate' ) > -1"
        },
        "isTemplateInEditMode" : {
            "expression": "props.subPanelContext.selected && ctx.workflowDgmEditCtx.editObjectUids && ctx.workflowDgmEditCtx.editObjectUids.indexOf( props.subPanelContext.selected.uid ) > -1"
        }
