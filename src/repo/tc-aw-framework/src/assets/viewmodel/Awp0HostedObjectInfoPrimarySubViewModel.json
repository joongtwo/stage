{
    "schemaVersion": "1.0.0",
    "imports": [],
    "actions": {
        "startEditNative": {
            "actionType": "TcSoaService",
            "serviceName": "Internal-AWS2-2017-12-DataManagement",
            "method": "loadViewModelForEditing2",
            "inputData": {
                "inputs": [
                    {
                        "objs": ["{{data.vmo}}"],
                        "propertyNames": "{{data.propertyNamesRetrieved}}",
                        "isPessimisticLock": false
                    }
                ]
            },
            "outputData": {
                "lsd": "outputs[0].objLsds"
            },
            "policy": {
                "types": [
                    {
                        "name": "{{data.vmo.type}}",
                        "modifiers": [
                            {
                                "name": "includeIsModifiable",
                                "Value": "true"
                            }
                        ],
                        "properties": "{{data.propertyPolicyRetrieved}}"
                    }
                ]
            },
            "events": {
                "success": [
                    {
                        "name": "startEditNative.success",
                        "eventData": {
                            "context": "INFO_PANEL_CONTEXT",
                            "source": "toolAndInfoPanel"
                        }
                    }
                ],
                "failure": [
                    {
                        "name": "startEditNative.fail",
                        "eventData": {
                            "context": "INFO_PANEL_CONTEXT",
                            "source": "toolAndInfoPanel"
                        }
                    }
                ]
            }
        },
        "saveEditNative": {
            "actionType": "TcSoaService",
            "serviceName": "Internal-AWS2-2018-05-DataManagement",
            "method": "saveViewModelEditAndSubmitWorkflow2",
            "inputData": {
                "inputs": [
                    {
                        "object": "{{data.vmo}}",
                        "objLsds": "{{data.lsd}}",
                        "viewModelProperties": "{{function:getAllModifiedValues}}",
                        "isPessimisticLock": false,
                        "workflowData": {}
                    }
                ]
            },
            "events": {
                "success": [
                    {
                        "name": "saveEditNative.success",
                        "eventData": {
                            "context": "INFO_PANEL_CONTEXT",
                            "source": "toolAndInfoPanel"
                        }
                    }
                ],
                "failure": [
                    {
                        "name": "saveEditNative.fail",
                        "eventData": {
                            "context": "INFO_PANEL_CONTEXT",
                            "source": "toolAndInfoPanel"
                        }
                    }
                ]
            },
            "deps": "js/declUtils"
        },
        "startEdit": {
            "actionType": "Event",
            "method": "Event",
            "inputData": {
                "events": [
                    {
                        "name": "objInfo.startEdit"
                    }
                ]
            }
        },
        "save": {
            "actionType": "Event",
            "method": "Event",
            "inputData": {
                "events": [
                    {
                        "name": "objInfo.saveEdit"
                    }
                ]
            }
        }
    },
    "functions": {
        "getAllModifiedValues": {
            "functionName": "getAllModifiedValues",
            "parameters": [
                "{{data.vmo}}"
            ]
        }
    },
    "data": {
        "xrtState": {
            "initialValues": {},
            "meta": {
                "xrtVMO": {
                    "field": "true",
                    "type": "viewModelObject"
                }
            }
        }
    },
    "editHandlers": {
        "infoPanelEditHandler": {
            "context": "INFO_PANEL_CONTEXT",
            "dataSource": {
                "xrtVMO": "{{data.xrtState}}"
            }
        }
    },
    "messages": {
        "confirmLeave": {
            "messageType": "WARNING",
            "messageText": "{{i18n.navigationConfirmationSingle}}",
            "messageTextParams": [
                "{{data.vmo.props.object_string.dbValue}}"
            ],
            "navigationOptions": [
                {
                    "option": "Discard",
                    "text": "{{i18n.discard}}",
                    "action": ""
                },
                {
                    "option": "Save",
                    "text": "{{i18n.save}}",
                    "action": "save"
                }
            ]
        }
    },
    "conditions": {
        "isEditButtonVisible": {
            "expression": "ctx.aw_hosting_state.enableEditing && ctx.INFO_PANEL_CONTEXT && ctx.INFO_PANEL_CONTEXT.canEdit && !ctx.INFO_PANEL_CONTEXT._editing"
        },
        "isSaveButtonVisible": {
            "expression": "ctx.aw_hosting_state.enableEditing && ctx.INFO_PANEL_CONTEXT && ctx.INFO_PANEL_CONTEXT._editing"
        }
    },
    "i18n": {
        "Awp0EditGroup": [
            "ZeroCompileCommandBaseMessages"
        ],
        "save": [
            "XRTMessages"
        ],
        "navigationConfirmationSingle": [
            "XRTMessages"
        ],
        "discard": [
            "XRTMessages"
        ]
    }
}
