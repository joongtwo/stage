    "conditions": {
        "validVendorZipFile": {
            "expression": "data.vendorFileData.vendorValidZipFile == true"
        },
        "isImportVendorHierarchy": {
            "expression": "ctx.panelContext.mrlImportVendor === 'mrlImportVendorHierarchy'"
        },
        "isImportVendorData": {
            "expression": "ctx.panelContext.mrlImportVendor === 'mrlImportVendorData'"
        },
        "isOneVendorSelected": {
            "expression": "data.dataProviders.mrlVendorsDataProvider.selectedObjects.length === 1"
        },
        "noVendorsFound": {
            "expression": "data.totalFound === 0"
        },
        "hasSelectedVendorDisclaimer": {
            "expression": "conditions.isOneVendorSelected && data.dataProviders.mrlVendorsDataProvider.selectedObjects[0].props.disclaimerText.uiValue !== ''"
        },
        "isImportVendorHierarchyInProgress": {
            "expression": "data.isImportVendorHierarchyInProgress.dbValue === true"
        },
        "isImportVendorProductsInProgress": {
            "expression": "data.isImportVendorProductsInProgress.dbValue === true"
        },
        "isMapVendorProductsInProgress": {
            "expression": "data.isMapVendorProductsInProgress.dbValue === true"
        },
        "isImport3DModelsInProgress": {
            "expression": "data.isImport3DModelsInProgress.dbValue === true"
        },
        "showImportVendorHierarchySummanry": {
            "expression": "data.isImportVendorHierarchyCompleted.dbValue === true && !conditions.isImportVendorHierarchyInProgress"
        },
        "showImportVendorProductsSummanry": {
            "expression": "data.isImportVendorProductsCompleted.dbValue === true && !conditions.isImportVendorProductsInProgress"
        },
        "showMapVendorProductsSummanry": {
            "expression": "data.isMapVendorProductsCompleted.dbValue === true && !conditions.isMapVendorProductsInProgress"
        },
        "showImport3DModelsSummanry": {
            "expression": "data.isImport3DModelsCompleted.dbValue === true && !conditions.isImport3DModelsInProgress"
        },
        "isUploadAndUnZipFileInProgress": {
            "expression": "data.isUploadAndUnZipFileInProgress.dbValue === true"
        },
        "mrlShowProgessInfo": {
            "expression": "conditions.isUploadAndUnZipFileInProgress || conditions.isImportVendorHierarchyInProgress || conditions.isImportVendorProductsInProgress || conditions.isMapVendorProductsInProgress || conditions.isImport3DModelsInProgress"
        },
        "isImportVendorHierarchyLogFileCreated": {
            "expression": "data.logFileTicketOfImportVendorHierarchy !== ''"
        },
        "isImportVendorProductsLogFileCreated": {
            "expression": "data.logFileTicketOfImportVendorProducts !== ''"
        },
        "isMapVendorProductsLogFileCreated": {
            "expression": "data.logFileTicketOfMapVendorProducts !== ''"
        },
        "isImport3DModelsLogFileCreated": {
            "expression": "data.logFileTicketOfImport3DModels !== ''"
        },
        "isMapVendorProductsSelected": {
            "expression": "data.mapVendorData.dbValue === true"
        },
        "continueForMappingOfImportedProducts": {
            "expression": "conditions.isMapVendorProductsSelected && data.vendorProductsSuccessfullyImported > 0"
        },
        "isImport3DModelSelected": {
            "expression": "data.import3DModel.dbValue === true"
        },
        "continueForImport3DModelForImportedProducts": {
            "expression": "conditions.isImport3DModelSelected && data.vendorProductsSuccessfullyImported > 0"
        }
    },
