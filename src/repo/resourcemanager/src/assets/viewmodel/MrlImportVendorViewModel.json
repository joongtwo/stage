{
    "schemaVersion": "1.0.0",
    "lifecycleHooks": {
        "onMount": "initialize"
    },
    "actions": {
        "initialize": {
            "actionType": "batchJob",
            "steps": [
                {
                    "action": "getDefaultResourceTypes",
                    "condition": "conditions.isImportVendorData"
                }
            ]
        },
        "getDefaultResourceTypes": {
            "actionType": "JSFunction",
            "method": "getCreateDefaultResourceTypes",
            "outputData": {
                "defaultResourceTypes": ""
            },
            "deps": "js/MrmUtils"
        },
        "itemTypeSelectionChange": {
            "actionType": "JSFunction",
            "method": "getSelectedItemType",
            "inputData": {
                "data": "{{data}}"
            },
            "outputData": {
                "selectedItemType": ""
            },
            "deps": "js/mrlImportVendorService"
        },
        "startUploadAndUnzipFile": {
            "actionType": "batchJob",
            "steps": [
                {
                    "action": "setUploadAndUnzipFileInProgressAsTrue"
                },
                {
                    "action": "mrlStartProgressBar"
                },
                {
                    "action": "getTransientFileTicketsForUpload"
                }
            ]
        },
        "setUploadAndUnzipFileInProgressAsTrue": {
            "actionType": "Event",
            "method": "Event",
            "inputData": {
                "events": [
                    {
                        "name": "MrlImportVendor.setUploadAndUnzipFileInProgressInfo",
                        "eventData": {
                            "uploadAndUnZipFileInProgress": true
                        }
                    }
                ]
            }
        },
        "setUploadAndUnzipFileInProgressInfo": {
            "actionType": "JSFunction",
            "method": "setUploadAndUnzipFileInProgressInfo",
            "inputData": {
                "data": "{{data}}",
                "isUploadAndUnZipFileInProgress": "{{data.isUploadAndUnZipFileInProgress}}",
                "uploadAndUnZipFileInProgress": "{{data.eventMap['MrlImportVendor.setUploadAndUnzipFileInProgressInfo'].uploadAndUnZipFileInProgress}}"
            },
            "outputData": {
                "isUploadAndUnZipFileInProgress": "isUploadAndUnZipFileInProgress",
                "mrlProgressInfoText": "mrlProgressInfoText"
            },
            "deps": "js/mrlImportVendorService"
        },
        "mrlStartProgressBar": {
            "actionType": "Event",
            "method": "Event",
            "inputData": {
                "events": [
                    {
                        "name": "progress.start"
                    }
                ]
            }
        },
        "updateFileSelection": {
            "actionType": "JSFunction",
            "method": "updateFileSelection",
            "inputData": {
                "fileData": "{{parameters}}"
            },
            "outputData": {
                "vendorFileData": ""
            },
            "deps": "js/mrlImportVendorService"
        },
        "getTransientFileTicketsForUpload": {
            "actionType": "TcSoaService",
            "serviceName": "Core-2007-01-FileManagement",
            "method": "getTransientFileTicketsForUpload",
            "inputData": {
                "transientFileInfos": [
                    {
                        "fileName": "{{data.vendorFileData.vendorZipFileName}}",
                        "isBinary": true,
                        "deleteFlag": true
                    }
                ]
            },
            "outputData": {
                "vendorPackageFileTicket": "transientFileTicketInfos[0].ticket"
            },
            "events": {
                "success": [
                    {
                        "name": "updateVendorFormData",
                        "condition": "data.vendorPackageFileTicket !== ''"
                    }
                ],
                "failure": [
                    {
                        "name": "progress.end"
                    },
                    {
                        "name": "MrlImportVendor.setUploadAndUnzipFileInProgressInfo",
                        "eventData": {
                            "uploadAndUnZipFileInProgress": false
                        }
                    }
                ]
            },
            "actionMessages": {
                "failure": [
                    {
                        "message": "mrlImportVendorFailed"
                    }
                ]
            }
        },
        "updateVendorFormData": {
            "actionType": "JSFunction",
            "method": "updateVendorFormData",
            "inputData": {
                "formData": "{{data.vendorFileData.vendorZipFileFormData}}",
                "key": "fmsTicket",
                "value": "{{data.vendorPackageFileTicket}}"
            },
            "events": {
                "success": [
                    {
                        "name": "uploadVendorZipFile"
                    }
                ]
            },
            "deps": "js/mrlImportVendorService"
        },
        "uploadVendorZipFile": {
            "actionType": "RESTService",
            "serviceName": "",
            "method": "POST",
            "inputData": {
                "request": {
                    "method": "POST",
                    "url": "{{Constants.fmsUrl}}",
                    "headers": {
                        "Content-type": "undefined"
                    },
                    "data": "{{data.vendorFileData.vendorZipFileFormData}}"
                }
            },
            "events": {
                "success": [
                    {
                        "name": "unzipVendorZipFile"
                    }
                ],
                "failure": [
                    {
                        "name": "progress.end"
                    },
                    {
                        "name": "MrlImportVendor.setUploadAndUnzipFileInProgressInfo",
                        "eventData": {
                            "uploadAndUnZipFileInProgress": false
                        }
                    }
                ]
            }
        },
        "unzipVendorZipFile": {
            "actionType": "TcSoaService",
            "serviceName": "Internal-Manufacturing-2015-03-ResourceManagement",
            "method": "unzipGtcPackage",
            "inputData": {
                "transientFmsZipFileTicket": "{{data.vendorPackageFileTicket}}"
            },
            "events": {
                "success": [
                    {
                        "name": "mrlVendorsTableGrid.plTable.reload"
                    },
                    {
                        "name": "progress.end"
                    },
                    {
                        "name": "MrlImportVendor.setUploadAndUnzipFileInProgressInfo",
                        "eventData": {
                            "uploadAndUnZipFileInProgress": false
                        }
                    }
                ],
                "failure": [
                    {
                        "name": "progress.end"
                    },
                    {
                        "name": "MrlImportVendor.setUploadAndUnzipFileInProgressInfo",
                        "eventData": {
                            "uploadAndUnZipFileInProgress": false
                        }
                    }
                ]
            },
            "actionMessages": {
                "failure": [
                    {
                        "message": "mrlImportVendorFailed"
                    }
                ]
            }
        },
        "startImportVendorHierarchy": {
            "actionType": "batchJob",
            "steps": [
                {
                    "action": "resetImportVendorProductsFlags"
                },
                {
                    "action": "setImportVendorHierarchyInProgressAsTrue"
                },
                {
                    "action": "mrlStartProgressBar"
                },
                {
                    "action": "importVendorHierarchy"
                }
            ]
        },
        "startImportVendorProducts": {
            "actionType": "batchJob",
            "steps": [
                {
                    "action": "resetImportVendorProductsFlags"
                },
                {
                    "action": "setImportVendorProductsInProgressAsTrue"
                },
                {
                    "action": "mrlStartProgressBar"
                },
                {
                    "action": "importVendorProducts"
                }
            ]
        },
        "resetImportVendorHierarchyFlags": {
            "actionType": "JSFunction",
            "method": "resetImportVendorHierarchyFlags",
            "inputData": {
                "data": "{{data}}"
            },
            "outputData": {
                "isImportVendorHierarchyCompleted": "isImportVendorHierarchyCompleted"
            },
            "deps": "js/mrlImportVendorService"
        },
        "resetImportVendorProductsFlags": {
            "actionType": "JSFunction",
            "method": "resetImportVendorProductsFlags",
            "inputData": {
                "data": "{{data}}"
            },
            "outputData": {
                "isImportVendorProductsCompleted": "isImportVendorProductsCompleted",
                "isMapVendorProductsCompleted": "isMapVendorProductsCompleted",
                "isImport3DModelsCompleted": "isImport3DModelsCompleted"
            },
            "deps": "js/mrlImportVendorService"
        },
        "setImportVendorHierarchyInProgressAsTrue": {
            "actionType": "Event",
            "method": "Event",
            "inputData": {
                "events": [
                    {
                        "name": "MrlImportVendor.setImportVendorHierarchyInProgressInfo",
                        "eventData": {
                            "importVendorHierarchyInProgress": true
                        }
                    }
                ]
            }
        },
        "setImportVendorProductsInProgressAsTrue": {
            "actionType": "Event",
            "method": "Event",
            "inputData": {
                "events": [
                    {
                        "name": "MrlImportVendor.setImportVendorProductsInProgressInfo",
                        "eventData": {
                            "importVendorProductsInProgress": true
                        }
                    }
                ]
            }
        },
        "setImportVendorHierarchyInProgressInfo": {
            "actionType": "JSFunction",
            "method": "setImportVendorHierarchyInProgressInfo",
            "inputData": {
                "data": "{{data}}",
                "isImportVendorHierarchyInProgress": "{{data.isImportVendorHierarchyInProgress}}",
                "importVendorHierarchyInProgress": "{{data.eventMap['MrlImportVendor.setImportVendorHierarchyInProgressInfo'].importVendorHierarchyInProgress}}",
                "selectedVendorName": "{{data.dataProviders.mrlVendorsDataProvider.selectedObjects[0].props.vendorName.uiValue}}"
            },
            "outputData": {
                "isImportVendorHierarchyInProgress": "isImportVendorHierarchyInProgress",
                "mrlProgressInfoText": "mrlProgressInfoText",
                "mrlImportVendorSummary": "mrlImportVendorSummary"
            },
            "deps": "js/mrlImportVendorService"
        },
        "setImportVendorProductsInProgressInfo": {
            "actionType": "JSFunction",
            "method": "setImportVendorProductsInProgressInfo",
            "inputData": {
                "data": "{{data}}",
                "isImportVendorProductsInProgress": "{{data.isImportVendorProductsInProgress}}",
                "importVendorProductsInProgress": "{{data.eventMap['MrlImportVendor.setImportVendorProductsInProgressInfo'].importVendorProductsInProgress}}",
                "selectedVendorName": "{{data.dataProviders.mrlVendorsDataProvider.selectedObjects[0].props.vendorName.uiValue}}"
            },
            "outputData": {
                "isImportVendorProductsInProgress": "isImportVendorProductsInProgress",
                "mrlProgressInfoText": "mrlProgressInfoText",
                "mrlImportVendorSummary": "mrlImportVendorSummary"
            },
            "deps": "js/mrlImportVendorService"
        },
        "setMapVendorProductsInProgressInfo": {
            "actionType": "JSFunction",
            "method": "setMapVendorProductsInProgressInfo",
            "inputData": {
                "data": "{{data}}",
                "isMapVendorProductsInProgress": "{{data.isMapVendorProductsInProgress}}",
                "mapVendorProductsInProgress": "{{data.eventMap['MrlImportVendor.setMapVendorProductsInProgressInfo'].mapVendorProductsInProgress}}",
                "selectedVendorName": "{{data.dataProviders.mrlVendorsDataProvider.selectedObjects[0].props.vendorName.uiValue}}"
            },
            "outputData": {
                "isMapVendorProductsInProgress": "isMapVendorProductsInProgress",
                "mrlProgressInfoText": "mrlProgressInfoText"
            },
            "deps": "js/mrlImportVendorService"
        },
        "setImport3DModelsInProgressInfo": {
            "actionType": "JSFunction",
            "method": "setImport3DModelsInProgressInfo",
            "inputData": {
                "data": "{{data}}",
                "isImport3DModelsInProgress": "{{data.isImport3DModelsInProgress}}",
                "import3DModelsInProgress": "{{data.eventMap['MrlImportVendor.setImport3DModelsInProgressInfo'].import3DModelsInProgress}}",
                "selectedVendorName": "{{data.dataProviders.mrlVendorsDataProvider.selectedObjects[0].props.vendorName.uiValue}}"
            },
            "outputData": {
                "isImport3DModelsInProgress": "isImport3DModelsInProgress",
                "mrlProgressInfoText": "mrlProgressInfoText"
            },
            "deps": "js/mrlImportVendorService"
        },
        "importVendorProducts": {
            "actionType": "TcSoaService",
            "serviceName": "Internal-Manufacturing-2022-12-ResourceManagement",
            "method": "importVendorData",
            "inputData": {
                "importVendorDataIn":  {
                    "vendorPackageId":"{{data.dataProviders.mrlVendorsDataProvider.selectedObjects[0].props.vendorPackageId.uiValue}}",
                    "vendorPackageDirectory": "{{data.dataProviders.mrlVendorsDataProvider.selectedObjects[0].props.vendorCatalogRootDir.uiValue}}",
                    "doCheckVendorProducts":false,
                    "doImportVendorProducts":true,
                    "doMapVendorProducts":false,
                    "doImport3DModels":false,
                    "importVendorProductsInput":{
                        "classId": "{{data.dataProviders.mrlVendorsDataProvider.selectedObjects[0].props.vendorCatalogRootClassId.uiValue}}",
                        "productCount": "{{data.dataProviders.mrlVendorsDataProvider.selectedObjects[0].props.vendorProductCount.value}}",
                        "importOption": 2
                    },
                    "mapVendorProductsInput": {
                        "targetItemTypeName":"{{data.selectedItemType}}"
                    }
                }
            },
            "outputData": {
                "importVendorProductsModelObjects": "ServiceData.modelObjects",
                "importVendorProductsInfo":"importVendorProductsInfo",
                "vendorProductsSuccessfullyImported":"importVendorProductsInfo.successCount",
                "alreadyImportedProductCount":"importedProductCount",
                "alreadyMappedProductCount":"mappedProductCount",
                "logFileTicketOfImportVendorProducts": "importVendorProductsInfo.logFileInfo.ticket"
            },
            "events": {
                "success": [
                    {
                        "name": "MrlImportVendor.importVendorProductsCompleted",
                        "eventData": {
                            "isImportVendorProductsSucceed": true
                        }
                    },
                    {
                        "name": "MrlImportVendor.setImportVendorProductsInProgressInfo",
                        "eventData": {
                            "importVendorProductsInProgress": false
                        }
                    },
                    {
                        "name": "MrlImportVendor.setMapVendorProductsInProgressInfo",
                        "eventData": {
                            "mapVendorProductsInProgress": true
                        },
                        "condition": "conditions.continueForMappingOfImportedProducts"
                    },
                    {
                        "name": "MrlImportVendor.setImport3DModelsInProgressInfo",
                        "eventData": {
                            "import3DModelsInProgress": true
                        },
                        "condition": "!conditions.continueForMappingOfImportedProducts && conditions.continueForImport3DModelForImportedProducts"
                    },
                    {
                        "name": "progress.end",
                        "condition": "!(conditions.continueForMappingOfImportedProducts || conditions.continueForImport3DModelForImportedProducts)"
                    },
                    {
                        "name": "MrlImportVendor.mapVendorProducts",
                        "condition": "conditions.continueForMappingOfImportedProducts"
                    },
                    {
                        "name": "MrlImportVendor.import3DModelsForVendorProducts",
                        "condition": "!conditions.continueForMappingOfImportedProducts && conditions.continueForImport3DModelForImportedProducts"
                    }
                ],
                "failure": [
                    {
                        "name": "MrlImportVendor.importVendorProductsCompleted",
                        "eventData": {
                            "isImportVendorProductsSucceed": false
                        }
                    },
                    {
                        "name": "MrlImportVendor.setImportVendorProductsInProgressInfo",
                        "eventData": {
                            "importVendorProductsInProgress": false
                        }
                    },
                    {
                        "name": "progress.end"
                    }
                ]
            },
            "actionMessages": {
                "failure": [
                    {
                        "message": "mrlImportVendorFailed"
                    }
                ]
            }
        },
        "mapVendorProducts": {
            "actionType": "TcSoaService",
            "serviceName": "Internal-Manufacturing-2022-12-ResourceManagement",
            "method": "importVendorData",
            "inputData": {
                "importVendorDataIn":  {
                    "vendorPackageId":"{{data.dataProviders.mrlVendorsDataProvider.selectedObjects[0].props.vendorPackageId.uiValue}}",
                    "vendorPackageDirectory": "{{data.dataProviders.mrlVendorsDataProvider.selectedObjects[0].props.vendorCatalogRootDir.uiValue}}",
                    "doCheckVendorProducts":false,
                    "doImportVendorProducts":false,
                    "doMapVendorProducts":true,
                    "doImport3DModels":false,
                    "importVendorProductsInput":{
                        "classId": "{{data.dataProviders.mrlVendorsDataProvider.selectedObjects[0].props.vendorCatalogRootClassId.uiValue}}",
                        "productCount": "{{data.dataProviders.mrlVendorsDataProvider.selectedObjects[0].props.vendorProductCount.value}}",
                        "importOption": 2
                    },
                    "mapVendorProductsInput": {
                        "targetItemTypeName":"{{data.selectedItemType}}"
                    }
                }
            },
            "outputData": {
                "mapVendorProductsModelObjects": "ServiceData.modelObjects",
                "mapVendorProductsInfo":"mapVendorProductsInfo",
                "vendorProductsMappedSuccessfully":"mapVendorProductsInfo.successCount",
                "logFileTicketOfMapVendorProducts": "mapVendorProductsInfo.logFileInfo.ticket"
            },
            "events": {
                "success": [
                    {
                        "name": "MrlImportVendor.mapVendorProductsCompleted",
                        "eventData": {
                            "isMapVendorProductsSucceed": true
                        }
                    },
                    {
                        "name": "MrlImportVendor.setMapVendorProductsInProgressInfo",
                        "eventData": {
                            "mapVendorProductsInProgress": false
                        }
                    },
                    {
                        "name": "MrlImportVendor.setImport3DModelsInProgressInfo",
                        "eventData": {
                            "import3DModelsInProgress": true
                        },
                        "condition": "conditions.continueForImport3DModelForImportedProducts"
                    },
                    {
                        "name": "progress.end",
                        "condition": "!conditions.continueForImport3DModelForImportedProducts"
                    },
                    {
                        "name": "MrlImportVendor.import3DModelsForVendorProducts",
                        "condition": "conditions.continueForImport3DModelForImportedProducts"
                    }
                ],
                "failure": [
                    {
                        "name": "MrlImportVendor.mapVendorProductsCompleted",
                        "eventData": {
                            "isMapVendorProductsSucceed": false
                        }
                    },
                    {
                        "name": "MrlImportVendor.setMapVendorProductsInProgressInfo",
                        "eventData": {
                            "mapVendorProductsInProgress": false
                        }
                    },
                    {
                        "name": "progress.end"
                    }
                ]
            },
            "actionMessages": {
                "failure": [
                    {
                        "message": "mrlImportVendorFailed"
                    }
                ]
            }
        },
        "import3DModelsForVendorProducts": {
            "actionType": "TcSoaService",
            "serviceName": "Internal-Manufacturing-2022-12-ResourceManagement",
            "method": "importVendorData",
            "inputData": {
                "importVendorDataIn":  {
                    "vendorPackageId":"{{data.dataProviders.mrlVendorsDataProvider.selectedObjects[0].props.vendorPackageId.uiValue}}",
                    "vendorPackageDirectory": "{{data.dataProviders.mrlVendorsDataProvider.selectedObjects[0].props.vendorCatalogRootDir.uiValue}}",
                    "doCheckVendorProducts":false,
                    "doImportVendorProducts":false,
                    "doMapVendorProducts":false,
                    "doImport3DModels":true,
                    "importVendorProductsInput":{
                        "classId": "{{data.dataProviders.mrlVendorsDataProvider.selectedObjects[0].props.vendorCatalogRootClassId.uiValue}}",
                        "productCount": "{{data.dataProviders.mrlVendorsDataProvider.selectedObjects[0].props.vendorProductCount.value}}",
                        "importOption": 2
                    },
                    "mapVendorProductsInput": {
                        "targetItemTypeName":"{{data.selectedItemType}}"
                    }
                }
            },
            "outputData": {
                "import3DModelsInfo":"import3DModelsInfo",
                "imported3DModelsSuccessfully":"import3DModelsInfo.successCount",
                "logFileTicketOfImport3DModels": "import3DModelsInfo.logFileInfo.ticket"
            },
            "events": {
                "success": [
                    {
                        "name": "MrlImportVendor.import3DModelsCompleted",
                        "eventData": {
                            "isImport3DModelsSucceed": true
                        }
                    },
                    {
                        "name": "MrlImportVendor.setImport3DModelsInProgressInfo",
                        "eventData": {
                            "import3DModelsInProgress": false
                        }
                    },
                    {
                        "name": "progress.end"
                    }
                ],
                "failure": [
                    {
                        "name": "MrlImportVendor.import3DModelsCompleted",
                        "eventData": {
                            "isImport3DModelsSucceed": false
                        }
                    },
                    {
                        "name": "MrlImportVendor.setImport3DModelsInProgressInfo",
                        "eventData": {
                            "import3DModelsInProgress": false
                        }
                    },
                    {
                        "name": "progress.end"
                    }
                ]
            },
            "actionMessages": {
                "failure": [
                    {
                        "message": "mrlImportVendorFailed"
                    }
                ]
            }
        },
