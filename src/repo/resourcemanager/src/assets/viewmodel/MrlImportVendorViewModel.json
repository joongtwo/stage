        "loadVendorsFromServer": {
            "actionType": "TcSoaService",
            "serviceName": "Internal-Manufacturing-2016-09-ResourceManagement",
            "method": "getVendorCatalogInfo3",
            "inputData": {
                "catalogTypeFilter": "{{ctx.panelContext.catalogTypeFilter}}"
            },
            "outputData": {
                "catalogInfo": "{{function:getVendors}}",
                "vendorCatalogInfoErrors": "ServiceData.partialErrors"
            },
            "actionMessages": {
                "failure": [
                    {
                        "message": "mrlImportVendorFailed",
                        "condition": "!(data.vendorCatalogInfoErrors[0].errorValues[0].code === 300301 || data.vendorCatalogInfoErrors[0].errorValues[0].code === 300303)"
                    }
                ]
            },
            "deps": "js/mrlImportVendorService"
        },
