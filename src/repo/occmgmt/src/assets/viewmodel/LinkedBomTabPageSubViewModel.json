{
    "schemaVersion": "1.0.0",
    "imports": [
        "js/aw-panel-body.directive",
        "js/aw-panel-section.directive",
        "js/aw-default-cell.directive",
        "js/aw-list.directive"
    ],
    "actions": {
        
        "reveal": {
            "actionType": "dataProvider",
            "method": "getLinkedObjectProvider"
        },
        "getLinkedObject": {
            "actionType": "JSFunction",
            "method": "getRelatedModelObjects",
            "inputData": {
                "primaryObject" :"{{props.subPanelContext.occContext.openedObject}}",
                "providerName" :"{{props.subPanelContext.provider.dataProviderName}}"
            },
            "outputData": {
                "linkedModelObjectResults": ""
            },
            "deps": "js/cbaRelatedObjectService"
        }
    },
    "dataProviders": {
        "getLinkedObjectProvider": {
            "action": "getLinkedObject",
            "response": "{{data.linkedModelObjectResults}}",
            "totalFound": "{{data.linkedModelObjectResults.length}}",
            "selectionModelMode": "single"
        }
    },
    "onEvent": [
       
    ],
    "lifecycleHooks": {
        "onInit": "reveal"
    },
    "functions": { 
       
    },
    "i18n": {
    }
}