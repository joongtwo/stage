  "functions": {},
  "onEvent": [
    {
      "eventId": "ctm1.translationReady",
      "action": "Ctm1DownloadDataset",
      "cacheEventData": true
    },
    {
      "eventId": "ctm1.okayToDeliver",
      "action": "Ctm1ShowNewTransDelivery"
    },
    {
      "eventId": "ctm1.copyGraphicReferenceReady",
      "action": "Ctm1CopyGraphicReferenceAction",
      "cacheEventData": true
    }
  ],
  "messages": {
    "deliveryAlreadyCreatedWarning": {
      "messageType": "ERROR",
      "messageText": "{{i18n.deliveryAlreadyCreate}}"
    }
  },
  "conditions": {
    "ctm1VersionCondition12_2": {
      "expression": "ctx.tcSessionData.tcMajorVersion === 12 && ctx.tcSessionData.tcMinorVersion > 2"
    },
    "ctm1VersionCondition13": {
      "expression": "ctx.tcSessionData.tcMajorVersion > 12"
    },
    "importTranslationSoaAvailable": {
      "expression": "conditions.ctm1VersionCondition12_2 || conditions.ctm1VersionCondition13"
    },
    "importTranslationVisible": {
      "expression": "conditions.objectIsAnyTopic && conditions.importTranslationSoaAvailable"
    },
    "objectIsCtm1Topic": {
      "expression": "ctx.selected.modelType.typeHierarchyArray.indexOf( 'Ctm1Topic' ) > -1"
    },
    "objectIsTopic": {
      "expression": "ctx.selected.modelType.typeHierarchyArray.indexOf( 'DC_TopicRevision' ) > -1"
    },
    "objectIsGraphicOptionRevision": {
      "expression": "ctx.selected.modelType.typeHierarchyArray.indexOf( 'GraphicOptionRevision') > -1"
    },
    "objectIsAnyTopic": {
      "expression": "ctx.selected.modelType.typeHierarchyArray.indexOf( 'Ctm1Topic' ) > -1 || ctx.selected.modelType.typeHierarchyArray.indexOf( 'DC_TopicRevision' ) > -1"
    },
    "objectIsTranslationOrder": {
      "expression": "ctx.selected.modelType.typeHierarchyArray.indexOf( 'TranslationOrderRevision' ) > -1"
    },
    "objectIsTranslationDelivery": {
      "expression": "ctx.selected.modelType.typeHierarchyArray.indexOf( 'TranslatnDelvryRevision' ) > -1"
    },
    "objectIsTranslation": {
      "expression": "ctx.selected.modelType.typeHierarchyArray.indexOf( 'DCt_TranslationRevision' ) > -1"
    },
    "oneObjectIsSelected": {
      "expression": "!ctx.mselected || ctx.mselected.length === 1"
    },
    "canOpenInXMetaL": {
      "expression": "conditions.objectIsTopic || conditions.objectIsTranslation"
    },
    "canOpenInXMetaLFromContent": {
      "expression": "conditions.objectIsCtm1Topic && conditions.oneObjectIsSelected"
    },
    "canOpenInOxygen": {
      "expression": "(conditions.objectIsTopic || conditions.objectIsTranslation) && conditions.ctm1EnableOxygenIntegration"
    },
    "canOpenInOxygenFromContent": {
      "expression": "conditions.objectIsCtm1Topic && conditions.oneObjectIsSelected && conditions.ctm1EnableOxygenIntegration"
    },
    "ctm1EnableOxygenIntegration": {
      "expression": "ctx.preferences.ctm0EnableOxygenIntegration[0]==='true' || ctx.preferences.ctm0EnableOxygenIntegration[0]==='True' || ctx.preferences.ctm0EnableOxygenIntegration[0]==='TRUE' && ctx.tcSessionData.tcMajorVersion === 14 && ctx.tcSessionData.tcMinorVersion >= 1"
    },
    "ctm1ShowChildLinesView": {
      "expression": "ctx.parametersTableViewMode !== undefined && ctx.parametersTableViewMode.viewModeContext === 'showChildLines'"
    },
    "ctm1OnlyElementsSelectedInTreeMode": {
      "expression": "conditions.ctm1ShowChildLinesView && ctx.parametersTable.selectedElementsFromProxy.length > 0 && ctx.parametersTable.selectedObjects.length === ctx.parametersTable.selectedElementsFromProxy.length"
    },
    "ctm1IsShowingArm0GenerateTraceabilityMatrix": {
      "expression": {
        "$and": [
          {
            "$source": "conditions.ctm1IsCreateTracelinkCommandVisible",
            "$query": {
              "$eq": true
            }
          },
          {
            "$source": "conditions.objectIsAnyTopic",
            "$query": {
              "$eq": true
            }
          }
        ]
      }
    },
    "enableOpenContMgmtNotificationCmd": {
      "expression": "commandContext.group.groupName==='CONTMGMT'"
    },
    "ctm1IsShowingAqc0AddCharRepresentation": {
      "expression": {
        "$and": [
          {
            "$source": "conditions.ctm1IsAddCharRepVisible",
            "$query": {
              "$eq": true
            }
          },
          {
            "$source": "conditions.objectIsAnyTopic",
            "$query": {
              "$eq": true
            }
          }
        ]
      }
    },
    "ctm1IsShowingAtt1SetFreezePoint": {
      "expression": {
        "$and": [
          {
            "$source": "conditions.ctm1setFreezePointCommandVisibilityCond",
            "$query": {
              "$eq": true
            }
          },
          {
            "$source": "conditions.objectIsAnyTopic",
            "$query": {
              "$eq": true
            }
          }
        ]
      }
    },
    "ctm1IsShowingEvm1CreateRecipe": {
      "expression": {
        "$and": [
          {
            "$source": "conditions.ctm1createRecipeFromBomSelection",
            "$query": {
              "$eq": true
            }
          },
          {
            "$source": "conditions.objectIsAnyTopic",
            "$query": {
              "$eq": true
            }
          }
        ]
      }
    },
    "ctm1setFreezePointCommandVisibilityCond": {
      "expression": "conditions.ctm1IsTcReleaseAtLeast123 && conditions.ctm1InAceContext && ( ctx.selected.modelType.typeHierarchyArray.indexOf( 'Awb0Element' ) > -1 || conditions.ctm1OnlyElementsSelectedInTreeMode )  && conditions.ctm1projectModelNotSelected"
    },
    "ctm1IsTcReleaseAtLeast123": {
      "expression": "ctx.tcSessionData.tcMajorVersion > 12 || ctx.tcSessionData.tcMajorVersion === 12 && ctx.tcSessionData.tcMinorVersion >= 3"
    },
    "ctm1InAceContext": {
      "expression": "ctx.occmgmtContext.modelObject.modelType.typeHierarchyArray.indexOf('Awb0Element') > -1"
    },
    "ctm1projectModelNotSelected": {
      "expression": "ctx.occmgmtContext.modelObject.modelType.typeHierarchyArray.indexOf('Uml1ModelElement') === -1 && ctx.pselected.modelType.typeHierarchyArray.indexOf( 'Uml1ModelElement' ) === -1"
    },
    "ctm1createRecipeFromBomSelection": {
      "expression": "ctx.occmgmtContext && ctx.occmgmtContext.selectedModelObjects && ctx.occmgmtContext.selectedModelObjects.length > 0 && ctx.occmgmtContext.topElement.modelType.typeHierarchyArray.indexOf('Fgf0ApplicationModel') === -1  && conditions.ctm1isTcReleaseAtLeast122Version"
    },
    "ctm1isTcReleaseAtLeast122Version": {
      "expression": "ctx.tcSessionData.tcMajorVersion > 12 || ( ctx.tcSessionData.tcMajorVersion === 12 && ctx.tcSessionData.tcMinorVersion >= 2 )"
    },
    "ctm1IsAddCharRepVisible": {
      "expression": "ctx.mselected.length === 1 && conditions.ctm1IsOccStructure"
    },
    "ctm1IsOccStructure": {
      "expression": "ctx.selected.modelType.typeHierarchyArray.indexOf( 'Awb0Element' ) > -1 && ctx.sublocation.nameToken === 'com.siemens.splm.client.occmgmt:OccurrenceManagementSubLocation'"
    },
    "ctm1IsCreateTracelinkCommandVisible": {
      "expression": "(conditions.ctm1IsCreateTracelinkCommandVisibleAce || conditions.ctm1IsCreateTracelinkCommandVisibleNonAce)"
    },
    "ctm1IsCreateTracelinkCommandVisibleAce": {
      "expression": "ctx.occmgmtContext  && (ctx.selected.modelType.typeHierarchyArray.indexOf( 'Awb0ConditionalElement' ) > -1 || ctx.selected.modelType.typeHierarchyArray.indexOf( 'ItemRevision' ) > -1)"
    },
    "ctm1IsCreateTracelinkCommandVisibleNonAce": {
      "expression": "!ctx.occmgmtContext && ctx.visibleServerCommands.Arm0CreateTraceLink"
    },
    "ctm1FolderSelected": {
      "expression": "ctx.selected.modelType.typeHierarchyArray.indexOf( 'Folder' ) > -1 && ctx.sublocation.nameToken === 'com.siemens.splm.clientfx.tcui.xrt.objectNavigationSubLocation' && ctx.mselected.length === 1"
    },
    "isStructureObjectAnyTopic": {
      "expression": "ctx.occmgmtContext && ( ctx.occmgmtContext.selectedModelObjects[0].modelType.typeHierarchyArray.indexOf( 'Ctm1Topic' ) > -1 || ctx.occmgmtContext.selectedModelObjects[0].modelType.typeHierarchyArray.indexOf( 'DC_TopicRevision' ) > -1 )"
    },
    "isStructureObjectPM": {
      "expression": "ctx.occmgmtContext && ctx.occmgmtContext.selectedModelObjects[0].modelType.typeHierarchyArray.indexOf( 'Ctm1Topic' ) > -1 && (ctx.occmgmtContext.selectedModelObjects[0].props.awb0UnderlyingObjectType === undefined || ctx.occmgmtContext.selectedModelObjects[0].props.awb0UnderlyingObjectType.dbValues[0] === 'DC_SkdPMRevision' || ctx.occmgmtContext.selectedModelObjects[0].props.awb0UnderlyingObjectType.dbValues[0] === 'Civ0PM4Revision')"
    },
    "isStructureObjectDM": {
      "expression": "ctx.occmgmtContext && ctx.occmgmtContext.selectedModelObjects[0].modelType.typeHierarchyArray.indexOf( 'Ctm1Topic' ) > -1 && ( ctx.occmgmtContext.selectedModelObjects[0].props.awb0UnderlyingObjectType === undefined || ctx.occmgmtContext.selectedModelObjects[0].props.awb0UnderlyingObjectType.dbValues[0] === 'Civ0DM4Revision')"
    },
    "isStructureObjectDMOrPM": {
      "expression": "( conditions.isStructureObjectDM || conditions.isStructureObjectPM )"
    }
  },
