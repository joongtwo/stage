{
    "schemaVersion" : "1.0.0",
    "imports":
    [
        "js/aw-panel.directive",
        "js/aw-widget.directive"
    ],

    "actions":
    {

        "updateDataSource": {
            "actionType": "JSFunction",
            "method": "addHtmlPanelInDataSource",
            "inputData": {
                "data": "{{data}}",
                "type": "{{data.subPanelContext.xrtType}}",
                "panelID": "Ctm1SkdDataModuleCode"
            },
            "deps": "js/addObjectUtils"
        }
    },

    "data":
    {
        "revision__skdmodelic": {
            "dbValue": "",
            "displayName": "{{i18n.modelIdentification}}",
            "type": "STRING",
            "isRequired": "true",
            "isEditable": "true",
            "requiredText": "A-Z 0-9 {2,14}",
            "maxLength": "14"
        },
        "revision__skdsdc": {
            "dbValue": "",
            "displayName": "{{i18n.systemDifferenceCode}}",
            "type": "STRING",
            "isRequired": "false",
            "isEditable": "true",
            "requiredText": "A-Z 0-9 {1,4}",
            "maxLength": "4"
        },
        "revision__skdchapnum": {
            "dbValue": "",
            "displayName": "{{i18n.chapterNumber}}",
            "type": "STRING",
            "isRequired": "false",
            "isEditable": "true",
            "requiredText": "A-Z 0-9 {2,3}",
            "maxLength": "3"
        },
        "revision__skdsection": {
            "dbValue": "",
            "displayName": "{{i18n.sectionNumber}}",
            "type": "STRING",
            "isRequired": "false",
            "isEditable": "true",
            "requiredText": "A-Z 0-9 {1}",
            "maxLength": "1"
        },
        "revision__skdsubsection": {
            "dbValue": "",
            "displayName": "{{i18n.subsection}}",
            "type": "STRING",
            "isRequired": "false",
            "isEditable": "true",
            "requiredText": "A-Z 0-9 {1}",
            "maxLength": "1"
        },
        "revision__skdsubject": {
            "dbValue": "",
            "displayName": "{{i18n.subject}}",
            "type": "STRING",
            "isRequired": "false",
            "isEditable": "true",
            "requiredText": "A-Z 0-9 {2} | A-Z 0-9 {4}",
            "maxLength": "4"
        },
        "revision__skddiscode": {
            "dbValue": "",
            "displayName": "{{i18n.disassemblyCode}}",
            "type": "STRING",
            "isRequired": "true",
            "isEditable": "true",
            "requiredText": "A-Z 0-9 {2}",
            "maxLength": "2"
        },
        "revision__skddiscodev": {
            "dbValue": "",
            "displayName": "{{i18n.disassemblyCodeVariant}}",
            "type": "STRING",
            "isRequired": "true",
            "isEditable": "true",
            "requiredText": "A-Z 0-9 {1,3}",
            "maxLength": "3"
        },
        "revision__skdincode": {
            "dbValue": "",
            "displayName": "{{i18n.informationCode}}",
            "type": "STRING",
            "isRequired": "true",
            "isEditable": "true",
            "requiredText": "A-Z 0-9 {3}",
            "maxLength": "3"
        },
        "revision__skdincodev": {
            "dbValue": "",
            "displayName": "{{i18n.informationCodeVariant}}",
            "type": "STRING",
            "isRequired": "true",
            "isEditable": "true",
            "requiredText": "A-Z 0-9 {1}",
            "maxLength": "1"
        },
        "revision__skditemloc": {
            "dbValue": "",
            "displayName": "{{i18n.itemLocationCode}}",
            "type": "STRING",
            "isRequired": "true",
            "isEditable": "true",
            "requiredText": "A-Z 0-9 {1}",
            "maxLength": "1"
        },
        "revision__skdsupeqvc": {
            "dbValue": "",
            "displayName": "{{i18n.supportEquipmentVariantCode}}",
            "type": "STRING",
            "isRequired": "false",
            "isEditable": "true",
            "requiredText": "A-Z 0-9 {1,4}",
            "maxLength": "4"
        },
        "revision__skdecscs": {
            "dbValue": "",
            "displayName": "{{i18n.equipmentCatAndSubCatCode}}",
            "type": "STRING",
            "isRequired": "false",
            "isEditable": "true",
            "maxLength": "3",
            "requiredText": "A-Z 0-9 {2,3}"
        },
        "revision__skdeidc": {
            "dbValue": "",
            "displayName": "{{i18n.equipmentIdentificationCode}}",
            "type": "STRING",
            "isRequired": "false",
            "isEditable": "true",
            "maxLength": "2",
            "requiredText": "A-Z 0-9 {2}"
        },
        "revision__skdcidc": {
            "dbValue": "",
            "displayName": "{{i18n.componentIdentificationCode}}",
            "type": "STRING",
            "isRequired": "false",
            "isEditable": "true",
            "maxLength": "4",
            "requiredText": "A-Z 0-9 {2} | A-Z 0-9 {4}"
        }
    },

    "dataProviders":
    {

    },

    "lifecycleHooks":
    {
        "onMount": "updateDataSource",
        "onUpdate": {
            "action": "updateDataSource",
            "observers": [ "viewModel.data" ]
        }
    },

    "messages":
    {

    },

    "i18n":
    {
        "modelIdentification": ["ContentMgmtMessages"],
        "systemDifferenceCode": ["ContentMgmtMessages"],
        "chapterNumber": ["ContentMgmtMessages"],
        "sectionNumber": ["ContentMgmtMessages"],
        "subsection": ["ContentMgmtMessages"],
        "subject": ["ContentMgmtMessages"],
        "disassemblyCode": ["ContentMgmtMessages"],
        "disassemblyCodeVariant": ["ContentMgmtMessages"],
        "informationCode": ["ContentMgmtMessages"],
        "informationCodeVariant": ["ContentMgmtMessages"],
        "itemLocationCode": ["ContentMgmtMessages"],
        "supportEquipmentVariantCode": ["ContentMgmtMessages"],
        "equipmentCatAndSubCatCode": ["ContentMgmtMessages"],
        "equipmentIdentificationCode": ["ContentMgmtMessages"],
        "componentIdentificationCode": ["ContentMgmtMessages"]
    }
}
