        "revision__civ0ModelIC": {
            "dbValue": "",
            "displayName": "{{i18n.modelIdentification}}",
            "type": "STRING",
            "isRequired": "true",
            "isEditable": "true",
            "requiredText": "A-Z 0-9 {2,14}",
            "maxLength": "14"
        },
        "revision__civ0SDC": {
            "dbValue": "",
            "displayName": "{{i18n.systemDifferenceCode}}",
            "type": "STRING",
            "isRequired": "true",
            "isEditable": "true",
            "maxLength": "4",
            "requiredText": "A-Z 0-9 {1,4}"
        },
        "revision__civ0SystemCode": {
            "dbValue": "",
            "displayName": "{{i18n.systemCode}}",
            "type": "STRING",
            "isRequired": "true",
            "isEditable": "true",
            "maxLength": "3",
            "requiredText": "A-Z 0-9 {2,3}"
        },
        "revision__civ0SubSystemCode": {
            "dbValue": "",
            "displayName": "{{i18n.subSystemCode}}",
            "type": "STRING",
            "isRequired": "true",
            "isEditable": "true",
            "maxLength": "1",
            "requiredText": "A-Z 0-9 {1}"
