        "revision__civ0SubSubSystemCode": {
            "dbValue": "",
            "displayName": "{{i18n.subSubsystemCode}}",
            "type": "STRING",
            "isRequired": "true",
            "isEditable": "true",
            "maxLength": "1",
            "requiredText": "A-Z 0-9 {1}"
        },
        "revision__civ0AssemblyCode": {
            "dbValue": "",
            "displayName": "{{i18n.assemblyCode}}",
            "type": "STRING",
            "isRequired": "true",
            "isEditable": "true",
            "maxLength": "4",
            "requiredText": "A-Z 0-9 {2} | A-Z 0-9 {4}"
        },
        "revision__civ0DisCode": {
            "dbValue": "",
            "displayName": "{{i18n.disassemblyCode}}",
            "type": "STRING",
            "isRequired": "true",
            "isEditable": "true",
            "maxLength": "2",
            "requiredText": "A-Z 0-9 {2}"
        },
        "revision__civ0DisCodeV": {
            "dbValue": "",
            "displayName": "{{i18n.disassemblyCodeVariant}}",
            "type": "STRING",
            "isRequired": "true",
            "isEditable": "true",
            "maxLength": "3",
            "requiredText": "A-Z 0-9 {1,3}"
        },
        "revision__civ0InCode": {
            "dbValue": "",
            "displayName": "{{i18n.informationCode}}",
            "type": "STRING",
            "isRequired": "true",
            "isEditable": "true",
            "maxLength": "3",
            "requiredText": "A-Z 0-9 {3}"
        },
        "revision__civ0InCodeV": {
            "dbValue": "",
            "displayName": "{{i18n.informationCodeVariant}}",
            "type": "STRING",
            "isRequired": "true",
            "isEditable": "true",
            "maxLength": "1",
            "requiredText": "A-Z 0-9 {1}"
        },
        "revision__civ0ItemLoc": {
            "dbValue": "",
            "displayName": "{{i18n.itemLocationCode}}",
            "type": "STRING",
            "isRequired": "true",
            "isEditable": "true",
            "maxLength": "1",
            "requiredText": "[A|B|C|D|T|Z]"
