{
  "commands": {
    "Aw2dPageUp": {
      "iconId": "miscChevronRight",
      "title": "{{i18n.pageUp}}",
      "description": "{{i18n.Aw2dPageUpDesc}}"
    },
    "Aw2dPageDown": {
      "iconId": "miscChevronLeft",
      "title": "{{i18n.pageDown}}",
      "description": "{{i18n.Aw2dPageDownDesc}}"
    },
    "Aw2dPage": {
      "iconId": "cmdBlankIcon",
      "title": "{{i18n.currentPage}}",
      "template": {
        "text": "{{commandContext.currentPage}}",
        "class": "aw-2dviewerjs-pagenumber"
      },
      "description": "{{i18n.Aw2dPageDesc}}"
    },
    "Aw2dPan2D": {
      "iconId": "cmdPan",
      "isGroup": false,
      "title": "{{i18n.pan}}",
      "description": "{{i18n.Aw2dPan2DDesc}}"
    },
    "Aw2dZoom2D": {
      "iconId": "cmdZoom",
      "isGroup": false,
      "title": "{{i18n.zoom}}",
      "description": "{{i18n.Aw2dZoom2DDesc}}"
    },
    "Aw2dView2DNavigationGroup": {
      "iconId": "cmdPan",
      "isShuttle": true,
      "title": "{{i18n.2DNavigation}}",
      "description": "{{i18n.Aw2dView2DNavigationGroupDesc}}"
    },
    "Aw2dFit2D": {
      "iconId": "cmdFitToWindow",
      "isGroup": false,
      "title": "{{i18n.fit}}",
      "description": "{{i18n.Aw2dFit2DDesc}}"
    },
    "Aw2dRotateCW": {
      "iconId": "cmdRotate",
      "title": "{{i18n.rotateClockwise}}",
      "description": "{{i18n.rotateCWDesc}}"
    },
    "Aw2dRotateCCW": {
      "iconId": "cmdRotateCounterClockwise",
      "title": "{{i18n.rotateCounterclockwise}}",
      "description": "{{i18n.rotateCCWDesc}}"
    }
  },
  "commandHandlers": {
    "aw2dMarkupHighlightCommandHandler": {
      "id": "Awp0MarkupHighlight",
      "activeWhen": {
        "condition": "conditions.isTwoDViewer"
      },
      "visibleWhen": false
    },
    "aw2dPageUpCommandHandler": {
      "id": "Aw2dPageUp",
      "action": "pageUp",
      "activeWhen": true,
      "visibleWhen": {
        "condition": "conditions.pageVisible"
      },
      "enableWhen": {
        "condition": "conditions.pageUpEnabled"
      }
    },
    "aw2dPageDownCommandHandler": {
      "id": "Aw2dPageDown",
      "action": "pageDown",
      "activeWhen": true,
      "visibleWhen": {
        "condition": "conditions.pageVisible"
      },
      "enableWhen": {
        "condition": "conditions.pageDownEnabled"
      }
    },
    "aw2dPageCommandHandler": {
      "id": "Aw2dPage",
      "action": "page",
      "activeWhen": true,
      "visibleWhen": {
        "condition": "conditions.pageVisible"
      },
      "enableWhen": {
        "condition": "conditions.pageVisible"
      }
    },
    "aw2dPan2DCommandHandler": {
      "id": "Aw2dPan2D",
      "action": "pan2D",
      "activeWhen": true,
      "visibleWhen": {
        "condition": "conditions.isTwoDViewer"
      },
      "enableWhen": true,
      "selectWhen": {
        "condition": "conditions.isTwoDPanMode"
      }
    },
    "aw2dZoom2DCommandHandler": {
      "id": "Aw2dZoom2D",
      "action": "zoom2D",
      "activeWhen": true,
      "visibleWhen": {
        "condition": "conditions.isTwoDViewer"
      },
      "enableWhen": true,
      "selectWhen": {
        "condition": "conditions.isTwoDZoomMode"
      }
    },
    "aw2dView2DNavigationGroupCommandHandler": {
      "id": "Aw2dView2DNavigationGroup",
      "activeWhen": true,
      "enableWhen": true,
      "visibleWhen": {
        "condition": "conditions.isTwoDViewer"
      }
    },
    "aw2dFit2DCommandHandler": {
      "id": "Aw2dFit2D",
      "action": "fit2D",
      "activeWhen": true,
      "visibleWhen": {
        "condition": "conditions.isTwoDViewer"
      },
      "enableWhen": true,
      "selectWhen": false
    },
    "aw2dRotateCWHandler": {
      "id": "Aw2dRotateCW",
      "action": "Rotate2dCWAction",
      "activeWhen": true,
      "visibleWhen": {
        "condition": "conditions.rotate2dActive"
      }
    },
    "aw2dRotateCCWHandler": {
      "id": "Aw2dRotateCCW",
      "action": "Rotate2dCCWAction",
      "activeWhen": true,
      "visibleWhen": {
        "condition": "conditions.rotate2dActive"
      }
    }
  },
  "commandPlacements": {
    "aw2dPageUpCommandPlacement": {
      "id": "Aw2dPageUp",
      "uiAnchor": "aw_viewerRightSide",
      "priority": 2
    },
    "aw2dPageCommandPlacement": {
      "id": "Aw2dPage",
      "uiAnchor": "aw_viewerRightSide",
      "priority": 3
    },
    "aw2dPageDownCommandPlacement": {
      "id": "Aw2dPageDown",
      "uiAnchor": "aw_viewerRightSide",
      "priority": 4
    },
    "aw2dPan2DCommandPlacement": {
      "id": "Aw2dPan2D",
      "uiAnchor": "aw_universalViewer",
      "priority": 1,
      "parentGroupId": "Aw2dView2DNavigationGroup"
    },
    "aw2dZoom2DCommandPlacement": {
      "id": "Aw2dZoom2D",
      "uiAnchor": "aw_universalViewer",
      "priority": 2,
      "parentGroupId": "Aw2dView2DNavigationGroup"
    },
    "aw2dView2DNavigationGroupCommandPlacement": {
      "id": "Aw2dView2DNavigationGroup",
      "uiAnchor": "aw_universalViewer",
      "priority": 20
    },
    "aw2dFit2DCommandPlacement": {
      "id": "Aw2dFit2D",
      "uiAnchor": "aw_universalViewer",
      "priority": 21
    },
    "aw2dRotateCCWPlacement": {
      "id": "Aw2dRotateCCW",
      "uiAnchor": "aw_universalViewer",
      "priority": 22
    },
    "aw2dRotateCWPlacement": {
      "id": "Aw2dRotateCW",
      "uiAnchor": "aw_universalViewer",
      "priority": 23
    }
  },
  "actions": {
    "pageUp": {
      "actionType": "JSFunction",
      "method": "pageUp",
      "inputData": {
        "commandContext": "{{commandContext}}"
      },
      "deps": "js/utils2dViewer"
    },
    "pageDown": {
      "actionType": "JSFunction",
      "method": "pageDown",
      "inputData": {
        "commandContext": "{{commandContext}}"
      },
      "deps": "js/utils2dViewer"
    },
    "page": {
      "actionType": "popup",
      "inputData": {
        "options": {
          "view": "Awp02dViewerMultiPage",
          "clickOutsideToClose": true,
          "reference": "button[button-id='Aw2dPage']",
          "subPanelContext": "{{commandContext}}",
          "commandContext": "{{commandContext}}"
        }
      }
    },
    "pan2D": {
      "actionType": "JSFunction",
      "method": "pan2D",
      "inputData": {
        "commandContext": "{{commandContext}}"
      },
      "deps": "js/utils2dViewer"
    },
    "zoom2D": {
      "actionType": "JSFunction",
      "method": "zoom2D",
      "inputData": {
        "commandContext": "{{commandContext}}"
      },
      "deps": "js/utils2dViewer"
    },
    "fit2D": {
      "actionType": "JSFunction",
      "method": "fit2D",
      "inputData": {},
      "deps": "js/utils2dViewer"
    },
    "Rotate2dCWAction": {
      "actionType": "JSFunction",
      "method": "rotateCW",
      "deps": "js/utils2dViewer"
    },
    "Rotate2dCCWAction": {
      "actionType": "JSFunction",
      "method": "rotateCCW",
      "deps": "js/utils2dViewer"
    }
  },
  "conditions": {
    "isTwoDZoomMode": {
      "expression": "commandContext.navMode === 'zoom'"
    },
    "isTwoDPanMode": {
      "expression": "commandContext.navMode === 'pan'"
    },
    "isTwoDViewer": {
      "expression": "commandContext.type === 'aw-2d-viewer'"
    },
    "pageVisible": {
      "expression": "conditions.isMultiPage && conditions.isTwoDViewer"
    },
    "isMultiPage": {
      "expression": "commandContext.numPages>1 && commandContext.currentPage>0"
    },
    "isFirstPage": {
      "expression": "commandContext.currentPage===1"
    },
    "isLastPage": {
      "expression": "commandContext.currentPage===commandContext.numPages"
    },
    "pageUpEnabled": {
      "expression": "conditions.pageVisible && !conditions.isLastPage"
    },
    "pageDownEnabled": {
      "expression": "conditions.pageVisible && !conditions.isFirstPage"
    },
    "rotate2dActive": {
      "expression": "conditions.isTwoDViewer"
    }
  },
  "i18n": {
    "pageUp": [
      "Viewer2DMessages"
    ],
    "pageDown": [
      "Viewer2DMessages"
    ],
    "currentPage": [
      "Viewer2DMessages"
    ],
    "pan": [
      "Viewer2DMessages"
    ],
    "zoom": [
      "Viewer2DMessages"
    ],
    "2DNavigation": [
      "Viewer2DMessages"
    ],
    "fit": [
      "Viewer2DMessages"
    ],
    "Aw2dPageUpDesc": [
      "Viewer2DMessages"
    ],
    "Aw2dPageDownDesc": [
      "Viewer2DMessages"
    ],
    "Aw2dPageDesc": [
      "Viewer2DMessages"
    ],
    "Aw2dPan2DDesc": [
      "Viewer2DMessages"
    ],
    "Aw2dZoom2DDesc": [
      "Viewer2DMessages"
    ],
    "Aw2dView2DNavigationGroupDesc": [
      "Viewer2DMessages"
    ],
    "Aw2dFit2DDesc": [
      "Viewer2DMessages"
    ]
  },
  "messages": {},
  "onEvent": []
}
