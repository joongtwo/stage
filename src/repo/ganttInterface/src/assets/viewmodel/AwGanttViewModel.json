{
    "schemaVersion": "1.0.0",
    "imports": [
        "js/aw-flex-column.directive"
    ],
    "actions": {
        "load":
        {
            "actionType": "JSFunction",
            "method": "loadGantt",
            "inputData":
            {
                "data": "{{data}}",
                "subPanelContextInfo": "{{props.subPanelContext}}"
            },
            "deps": "js/Gantt"
        },
        "unload": {
            "actionType": "JSFunction",
            "method": "unLoadGantt",
            "inputData": {},
            "deps": "js/Gantt"
        }
    },
    "props": {
        "sub-panel-context": {
            "type": "object"
        }
    },
    "lifecycleHooks": {
        "onMount": "load",
        "onUnmount": "unload"
    },
    "conditions": {
        "showArrangeColumns": {
            "expression": "!subPanelContext.viewModel.showArrangeColumn || subPanelContext.viewModel.showArrangeColumn == 'true'"
        }
    },
